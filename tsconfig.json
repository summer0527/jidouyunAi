{
  "compilerOptions": {
    "module": "es2020",
    "target": "es6",
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "moduleResolution": "node",
    "strictNullChecks": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "types": ["unplugin-vue-macros/macros-global", "node","puppeteer"],
    "paths": {
      "ant-design-x-vue": ["src/index.ts"],
      "ant-design-x-vue/*": ["src/*"],
      "@/*": ["src/*"]
    }

    
  },
  "vueCompilerOptions": {
    "plugins": ["unplugin-vue-macros/volar"]
  },
  "include": ["src/**/*", "docs/**/*", "internal/**/*", "custom.d.ts", "src/**/*.ts",
  "src/types/*.d.ts", "src/**/*.ts","src/**/*.d.ts", "env.d.ts"],
}
